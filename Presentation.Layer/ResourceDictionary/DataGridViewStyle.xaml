            </Trigger>

        </Style.Triggers>
    </Style>

    <Style x:Key="DashboardCellStyleSL" TargetType="DataGridCell" BasedOn="{StaticResource DashboardCellStyle}">
        <Setter Property="TextBlock.TextWrapping" Value="Wrap"/>
        <Setter Property="Height" Value="70"/>
        <Setter Property="FontFamily" Value="sans-serif"/>
        <Setter Property="FontWeight" Value="SemiBold"/>
    </Style>


    <Style x:Key="DashboardRightCellStyle" TargetType="{x:Type DataGridCell}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Padding" Value="15 0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type DataGridCell}">
                    <Border CornerRadius="10" Background="{TemplateBinding Background}">
                        <ContentPresenter Margin="{TemplateBinding Padding}" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="#DCDCDC" />
                <Setter Property="Foreground" Value="#343232" />
                <Setter Property="BorderThickness" Value="0" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="DashboardCenterCellStyle" TargetType="{x:Type DataGridCell}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type DataGridCell}">
                    <Border CornerRadius="0" Background="{TemplateBinding Background}">
                        <ContentPresenter Margin="{TemplateBinding Padding}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="#1e88e5" />
                <Setter Property="Foreground" Value="White" />
                <Setter Property="BorderThickness" Value="0" />
            </Trigger>
        </Style.Triggers>
    </Style>


    <Style x:Key="DashboardDataGridStyle" TargetType="DataGrid">
        <Setter Property="CellStyle" Value="{StaticResource DashboardCellStyle}"/>
        <Setter Property="RowStyle" Value="{StaticResource DashboardRowStyle}"/>
        <Setter Property="ColumnHeaderStyle" Value="{StaticResource DashboardHeaderStyle}"/>
    </Style>
</ResourceDictionary>