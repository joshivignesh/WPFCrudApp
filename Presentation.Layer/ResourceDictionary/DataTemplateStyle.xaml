                    xmlns:ctrl="clr-namespace:Presentation.Layer.Control"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate DataType="{x:Type vm:UserViewModel}">
        <view:ucUser/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomMessageBoxVM}">
